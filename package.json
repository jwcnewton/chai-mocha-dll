{
  "name": "chai-mocha-project",
  "version": "1.0.0",
  "description": "DLL June",
  "main": "index.js",
  "standard": {
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "scripts": {
    "start": "node index.js",
    "test": "mocha --require babel-polyfill --compilers js:babel-register ./specs/**/*.spec.js",
    "compile": "babel src --out-file script-compiled.js",
    "cover": "istanbul cover _mocha --require babel-polyfill --compilers js:babel-register ./specs/**/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "author": "Jonathan Newton",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.0.2",
    "gulp": " 3.9.1",
    "gulp-concat": "2.6.1 ",
    "gulp-sourcemaps": "2.6.0",
    "mocha": "^3.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "4.0.2",
    "coveralls": "2.13.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-google": "^0.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp-babel": "^6.1.2",
    "istanbul": "0.4.5",
    "mocha": "3.4.2",
    "mocha-lcov-reporter": "1.3.0"
  }
}
